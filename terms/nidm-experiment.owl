              rdfs:label "T2 Star Weighted"^^xsd:string ;
                    
              rdfs:subClassOf nidm:ImageContrastType ;

              obo:IAO_0000115 "T2 star weighted is an image contrast type in which differences in voxel intensity reflect differences in the spin-spin relaxation time  of the probe as well as the field inhomogeneities in the underlying sample."^^xsd:string ;
             
              obo:IAO_0000114 obo:IAO_0000428 .



