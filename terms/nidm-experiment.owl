nidm:SinglePhotonEmissionComputedTomographyScanner rdf:type owl:Class ;
                                
              rdfs:label "Single Photon Emission Computed Tomography Scanner" ;
                                
              rdfs:subClassOf nidm:ImageAcquisitionDevice ;

