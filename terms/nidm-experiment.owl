              rdfs:label "Stimulus Presentation File"^^xsd:string ;
                              
              rdfs:subClassOf nidm:AuxiliaryFile ;
                              
              obo:IAO_0000115 "A stimulus presentation file is an auxiliary files that contains the presentation timing and/or location of the experimental stimulus (e.g., words, pictures) that are shown to the study participant in a task-based assessment activity."^^xsd:string ;
                                
              obo:IAO_0000114 obo:IAO_0000428 .



