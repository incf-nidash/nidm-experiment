nidm:SpinEcho rdf:type owl:Class ;
                                
               rdfs:label "Spin Echo"^^xsd:string ;

               obo:IAO_0000115 "Spin echo is a magnetic resonance imaging acquisition method in which the echo used in the measurement is generated by application of an initial radiofrequency pulse and a subsequent pulse, generally, though not necessarily, with flip angles of 90 and 180 degrees respectively."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "SE"^^xsd:string ;
                                
               rdfs:subClassOf nidm:AcquisitionMethod .



