###  http://purl.org/dc/terms/title

dct:title rdf:type owl:DatatypeProperty .



###  http://purl.org/dc/terms/description

dct:description rdf:type owl:DatatypeProperty ;
                
               rdfs:subPropertyOf owl:topDataProperty .



###  http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#fileName

nfo:fileName rdf:type owl:DatatypeProperty .


###  http://purl.org/dc/terms/license

dct:license rdf:type owl:DatatypeProperty .



###  http://purl.org/dc/terms/source

dct:source rdf:type owl:DatatypeProperty .



###  http://purl.org/nidash/nidm#hadNumericalValue

nidm:hadNumericalValue rdf:type owl:DatatypeProperty .



###  http://purl.org/nidash/nidm#bathSolution

nidm:bathSolution rdf:type owl:DatatypeProperty ;
                          
               rdfs:label "Bath Solution"^^xsd:string ;
                          
               obo:IAO_0000115 "A bath solution is a property that associates a string describing the solution that engulfs a sample with the data acquisition object obtained from that sample."^^xsd:string ;
                          
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#cellType

nidm:cellType rdf:type owl:DatatypeProperty ;
                       
               rdfs:label "Cell Type"^^xsd:string ;
                       
               obo:IAO_0000115 "Cell type is property that associates a string that is the name of the cells on which measurements are made, with a data acquisition object."^^xsd:string ;
                       
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#channelNumber

nidm:channelNumber rdf:type owl:DatatypeProperty ;
                       
               rdfs:label "Channel Number"^^xsd:string ;
                       
               obo:IAO_0000115 "Channel number is property that associates the numeric label of a data acquisition channel of a device to the data acquisition object that was acquired using that device."^^xsd:string ;
                       
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#electrodeImpedance

nidm:electrodeImpedance rdf:type owl:DatatypeProperty ;
                       
               rdfs:label "Electrode Impedance"^^xsd:string ;
                       
               obo:IAO_0000115 "Electrode impedance is property that associates the numeric value of the ratio of the voltage across the electrodes to the measured AC current (impedance) with the data acquisition object that was acquired."^^xsd:string ;
                       
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#hadFilePath
nidm:hadFilePath rdf:type owl:DatatypeProperty ;
                          
               rdfs:label "had File Path"^^xsd:string ;
                          
               obo:IAO_0000115 "Had file path is a datatype property that associates a file on a computor storage volume with the string containing the location of that file on the computer storage volume."^^xsd:string ;
                          
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#hadPath
nidm:hadPath rdf:type owl:DatatypeProperty ;
                          
               rdfs:label "had Path"^^xsd:string ;
                          
               obo:IAO_0000115 "Had path is a datatype property that associates an entity on a computor storage volume with the string containing the location of that entity on the computer storage volume."^^xsd:string ;

	        rdfs:comment "This can be used for entities other than files, such as folders and symbolic links."^^xsd:string ;
                          
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#groupLabel

nidm:groupLabel rdf:type owl:DatatypeProperty ;

               rdfs:label "Group Label"^^xsd:string ;

               obo:IAO_0000115 "Group label is a property that associates the label used to denote membership in a particular collection with a person or sample."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#hollowElectrodeSolution

nidm:hollowElectrodeSolution rdf:type owl:DatatypeProperty ;
                          
               rdfs:label "Hollow Electrode Solution"^^xsd:string ;
                          
               obo:IAO_0000115 "A hollow electrode solution is a property that associates a string describing the solution that is placed in a hollow electrode used to make an electrophysiology measurement with the data acquisition object obtained from that sample."^^xsd:string ;
                          
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#hadImageContrastType

nidm:hadImageContrastType rdf:type owl:DatatypeProperty ;
                          
               rdfs:label "had Image Contrast Type"^^xsd:string ;
                          
               obo:IAO_0000115 "Had image contrast type is a property that associates a string that describes the physical parameter that was manipulated to achieve the contrast of a given image with that image data acquisition object."^^xsd:string ;
                          
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#hadImageUsageType

nidm:hadImageUsageType rdf:type owl:DatatypeProperty ;
                       
               rdfs:label "had Image Usage Type"^^xsd:string ;
                       
               obo:IAO_0000115 "Had Image usage type is a property that associates a string that describes the purpose for which an image was acquired with the image data acquisition object."^^xsd:string ;
                       
               obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/nidm#numberOfChannels

nidm:numberOfChannels rdf:type owl:DatatypeProperty ;
                       
               rdfs:label "Number Of Channels"^^xsd:string ;
                       
               obo:IAO_0000115 "Number of channels is a property that associates the number of data acquisition meassurenments concurrently acquired by a data acquisition device to the data acquisition object acquired by that device."^^xsd:string ;
                       
               obo:IAO_0000116 "To be discussed"^^xsd:string .


           
###  http://purl.org/nidash/nidm#appliedFilter

nidm:appliedFilter rdf:type owl:DatatypeProperty ;

               rdfs:label "Applied Filter"^^xsd:string ;

               obo:IAO_0000115 "Applied filter is a property that associates the string name of the filter type used to remove or modify certain components of the input signal with a data acquisition object that includes that signal."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 .

           

###  http://purl.org/nidash/nidm#solutionFlowSpeed

nidm:solutionFlowSpeed rdf:type owl:DatatypeProperty ;

               rdfs:label "Solution Flow Speed"^^xsd:string ;

               obo:IAO_0000115 "Solution flow speed is a property that associates the value of the speed at which the bathing solution moves across the sample with a data acquisition object."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 .

           

###  http://purl.org/nidash/nidm#recordingLocation

nidm:recordingLocation rdf:type owl:DatatypeProperty ;

               rdfs:label "Recording Location"^^xsd:string ;

               obo:IAO_0000115 "Recording location is a property that associates a string that describes the anatomical location at which the electrophysiological measurement was made with a data acquisition object."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#DICOMTag

nidm:DICOMTag rdf:type owl:DatatypeProperty ;

               rdfs:label "DICOM Tag"^^xsd:string ;

               obo:IAO_0000115 "DICOM tag is a property that associates a string that denotes a specific DICOM standard tag identifier with a data acquisition object."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 .



#################################################################
#
#    Classes
#
#################################################################



###  http://purl.org/nidash/nidm#Acquisition

nidm:Acquisition rdf:type owl:Class ;

               rdfs:label "Acquisition" ;

               obo:IAO_0000115 "An acquisition is an activty in which data about a study participant is gathered."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                 
               rdfs:subClassOf prov:Activity .




###  http://purl.org/nidash/nidm#AcquisitionDeviceOperator

nidm:AcquisitionDeviceOperator rdf:type owl:Class ;
                                  
               rdfs:label "Acquisition Device Operator" ;

               obo:IAO_0000115 "An acquisition device operator is a role of an agent who/that controls the device that collects the acquisition object."^^xsd:string ;
                        
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "The use of who/that indicates that the agent performing this role can either be a person or another device." ;
                                  
               rdfs:subClassOf prov:Role .




###  http://purl.org/nidash/nidm#AcquisitionMethod

nidm:AcquisitionMethod rdf:type owl:Class ;
                       
               rdfs:label "Acquisition Method" ;

               obo:IAO_0000115 "An acquisition method is a specific form of a procedure that was used to gather data from a study participant."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                       
               rdfs:subClassOf nidm:AcquisitionObjectQuality .



###  http://purl.org/nidash/nidm#AcquisitionModality

nidm:AcquisitionModality rdf:type owl:Class ;
                         
               rdfs:label "Acquisition Modality" ;

               obo:IAO_0000115 "An acquisition modality is a description of the general type of experiment that was used to gather data from a study participant."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                         
               rdfs:subClassOf nidm:AcquisitionObjectQuality .



###  http://purl.org/nidash/nidm#AcquisitionObject

nidm:AcquisitionObject rdf:type owl:Class ;
                       
               rdfs:label "Acquisition Object"^^xsd:string ;
                       
               rdfs:subClassOf prov:Entity ;
                       
               obo:IAO_0000115 "An acquisition object is an entity that is gathered from a subject during an acquisition activity."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                       
               obo:IAO_0000117 <http://purl.org/nicholsn/card.ttl> .



###  http://purl.org/nidash/nidm#AcquisitionObjectQuality

nidm:AcquisitionObjectQuality rdf:type owl:Class ;

               rdfs:label "Acquisition Object Quality"^^xsd:string ;

               obo:IAO_0000115 "An acquisition object quality is an quality that describes an essential feature of an object that is gathered from a study participant during an acquisition activity."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                              
               rdfs:subClassOf obo:PATO_0000001 .



###  http://purl.org/nidash/nidm#AcquisitionUsageType

nidm:AcquisitionUsageType rdf:type owl:Class ;
                    
               rdfs:label "Acquisition Usage Type"^^xsd:string ;

               obo:IAO_0000115 "Acquisition usage type is an acquisition object quality that denotes to what purpose the acquired object will be employed."^^xsd:string ;
                     
               rdfs:comment  "This is used for a non-imaging based acquisition modality."^^xsd:string ;
  
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:subClassOf nidm:AcquisitionObjectQuality .



###  http://purl.org/nidash/nidm#Amperometry

nidm:Amperometry rdf:type owl:Class ;
                 
               rdfs:label "Amperometry" ;

               obo:IAO_0000115 "Amperometry is an extracellular acquisition method in electrophysiology in which an electrode is brought near a cell to detect the release of ions by vesicles."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                 
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Anatomical

nidm:Anatomical rdf:type owl:Class ;

               rdfs:label "Anatomical" ;

               obo:IAO_0000115 "Anatomical is a description of the usage of a given acquisition object, indicating that the purpose of the object is to provide information on the anatomy of the object on which the acquisition was performed."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                
               rdfs:subClassOf nidm:ImageUsageType .



###  http://purl.org/nidash/nidm#Angiography

nidm:Angiography rdf:type owl:Class ;
                                   
               rdfs:label "Angiography"^^xsd:string ;

               obo:IAO_0000115 "Angiography is an acquisition method that visualizes the venus and arterial systems of the body."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       rdfs:comment "Angiography is a general term for a method that exists in several image-based modalities such as magnetic resonance imaging."^^xsd:string ;
                                   
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#ArterialSpinLabeling

nidm:ArterialSpinLabeling rdf:type owl:Class ;
                                   
               rdfs:label "Arterial Spin Labeling"^^xsd:string ;

               obo:IAO_0000115 "Arterial spin labeling is a magnetic resonance imaging acquisition method that measures perfusion using the flow of saturated blood into a predefined area by comparing the image intensity obtained in the slice with and without the saturation pulse."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "ASL"^^xsd:string ;
                                   
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Attenuator

nidm:Attenuator rdf:type owl:Class ;
                  
               rdfs:label "Attenuator" ;

               rdfs:subClassOf nidm:DataProcessingDevice ;

               obo:IAO_0000115 "An attenuator is a device that reduces the amplitude of an input signal by a fixed amount while keeping the timecourse of the signal essentially unchanged." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .                    



###  http://purl.org/nidash/nidm#AuxiliaryFile

nidm:AuxiliaryFile rdf:type owl:Class ;
                   
               rdfs:label "AuxiliaryFile"^^xsd:string ;
                   
               rdfs:subClassOf sio:SIO_000396 ;
                   
               obo:IAO_0000115 "An auxiliary file is an information-bearing object that is either used to inform the execution of an experiment or that is generated during the execution of the experiment."^^xsd:string ;

               rdfs:comment "An example of a file that informs the execution of an experiment is a b-vector file in diffusion-weighted MR imaging.  An example of a file that is generated during the execution of the experiment is a stimulus response file in functional MR imaging)"^^xsd:string ;
                   
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#AuxiliaryFileCollection

nidm:AuxiliaryFileCollection rdf:type owl:Class ;
                             
               rdfs:label "Auxiliary File Collection"^^xsd:string ;
                             
               rdfs:subClassOf prov:Collection ;
                             
               obo:IAO_0000115 "An auxiliary file collection is a grouping that contains the URI's of files that are necessary for the execution of the experiment or the processing of the data."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "An example of an auxiliary file collection is the b-value and b-vector files in diffusion-weighted MR imaging."^^xsd:string ;
                             
               prov:wasAssociatedWith nidm:AcquisitionObject .



###  http://purl.org/nidash/nidm#BandpassFilter

nidm:BandpassFilter rdf:type owl:Class ;
                  
               rdfs:label "Bandpass Filter" ;

               rdfs:subClassOf nidm:SignalFilter ;

               obo:IAO_0000115 "A bandpass filter is a device that modifies or removes certain signal components outside a selected frequency range from an electrical signal." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .                    



###  http://purl.org/nidash/nidm#BehavioralInstrument

nidm:BehavioralInstrument rdf:type owl:Class ;

               rdfs:label "Behavioral Instrument"^^xsd:string ;

               obo:IAO_0000115 "A behavioral instrument is a instrument usage type that signifies that the type of data to be collected by the instrument is behaviorally-related data."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;
               
               rdfs:subClassOf nidm:InstrumentUsageType .



###  http://purl.org/nidash/nidm#NIDM_0000152

nidm:NIDM_0000152 rdf:type owl:Class ;
                  
               rdfs:label "Block-Based Design"^^xsd:string ;

               rdfs:subClassOf nidm:NIDM_0000155 ;
                  
               obo:IAO_0000115 "A block-based design is a fMRI design comprised of epochs of stimulus and/or behavior."^^xsd:string ;
                  
               obo:IAO_0000116 "Discussed at https://github.com/incf-nidash/nidm/issues/299"^^xsd:string ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#BloodDraw

nidm:BloodDraw rdf:type owl:Class ;
                  
               rdfs:label "Blood Draw"^^xsd:string ;

               rdfs:subClassOf prov:Activity ;
                  
               obo:IAO_0000115 "Blood draw is an activity in which blood is removed from the body, usually for the purpose of medically-related or scientific measurements."^^xsd:string ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#BloodOxygenLevelDependentWeighted

nidm:BloodOxygenLevelDependentWeighted rdf:type owl:Class ;
                  
               rdfs:label "Blood Oxygen Level Dependent Weighted"^^xsd:string ;

               obo:IAO_0000115 "Blood Oxygen Level Dependent Weighted is an image contrast type in which differences in voxel intensity are based on magnetic susceptibility differences between oxygenated and de-oxygenated blood."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "BOLD"^^xsd:string ;

	       rdfs:comment "This term is a special case of nidm:SusceptibilityWeighted, in which the image contrast is primarily due to differences in magnetic susceptibility of oxygenated versus de-oxygenated blood. Susceptibility weighting can have numerous causes, such as differences in tissue iron content."^^xsd:string ;
                  
               rdfs:subClassOf nidm:ImageContrastType .



###  http://purl.org/nidash/nidm#CalculatedParameter

nidm:CalculatedParameter rdf:type owl:Class ;
                  
               rdfs:label "Calculated Parameter"^^xsd:string ;

               obo:IAO_0000115 "A Calculated Parameter is a variable that is the result of the processing of a set of measurements."^^xsd:string ;

               rdfs:comment "Note that this is similar to, but distinct from a dependent variable, which is a variable that changes in response to changes in a system.  The T2 value would be an example of a dependent variable since changing the echo time allows for calculation of the T2.  Cerebral Blood Flow (CBV) is an example of a Calculated Parameter since it is measure by taking a series of images after the injection of a contrast agent; however the experimenter is not actually changing any independent variable to see a change in the CBV and therefore it is a Calculated Parameter rather than a dependent variable."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:subClassOf sio:SIO_000367 .



###  http://purl.org/nidash/nidm#Cartesian

nidm:Cartesian rdf:type owl:Class ;

               rdfs:label "Cartesian"^^xsd:string ;

               obo:IAO_0000115 "Cartesian is a k-space traversal scheme in MRI in which the path of the data acquisition follows a straight line parallel to one axis of the defined k-space and perpendicular to the other k=space axis."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;
               
               rdfs:subClassOf nidm:k-spaceTraversalScheme ;

               owl:synonym nidm:Rectilinear .



###  http://purl.org/nidash/nidm#CerebralBloodFlow

nidm:CerebralBloodFlow rdf:type owl:Class ;
                  
               rdfs:label "Cerebral Blood Flow"^^xsd:string ;

               obo:IAO_0000115 "Cerebral Blood Flow (CBF) is a calculated parameter that characterizes the blood volume in the brain."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;

	       rdfs:comment "This calculated parameter is usually displayed as a map that displays the CBF in quantitative or relative units."^^xsd:string ;

	       nidm:hasAcronym "CBF"^^xsd:string ;
                  
               rdfs:subClassOf nidm:CalculatedParameter .



###  http://purl.org/nidash/nidm#CerebralBloodVolume

nidm:CerebralBloodVolume rdf:type owl:Class ;
                  
               rdfs:label "Cerebral Blood Volume"^^xsd:string ;

               obo:IAO_0000115 "Cerebral Blood Volume (CBV) is a calculated parameter that characterizes the blood volume in the brain."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "CBV"^^xsd:string ;

	       rdfs:comment "This calculated parameter is usually displayed as a map that displays the CBV in quantitative or relative units."^^xsd:string ;
                  
               rdfs:subClassOf nidm:CalculatedParameter .



