nidm:ClinicalResearchCoordinator rdf:type owl:Class ;
                           
              rdfs:label "Clinical Research Coordinator"^^xsd:string ;
                           
              rdfs:subClassOf prov:Role ;
                           
              obo:IAO_0000115 "A clinical research coordinator is a role for a person who performs an activity during which clinical data is acquired from study participants."^^xsd:string ;
                           
              obo:IAO_0000114 obo:IAO_0000428 .



