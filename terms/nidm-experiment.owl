nidm:SteadyStateFreePrecession rdf:type owl:Class ;
                                   
               rdfs:label "Steady State Free Precession"^^xsd:string ;

               obo:IAO_0000115 "Steady State Free Precession is a magnetic resonance imaging acquisition method that uses the steady state magnetization, formed by a series of phase-alternated radiofrequency pulses, spoiler gradients in the slice-selection direction, rewinder gradients in the phase-encoding direction to form images."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "SSFP"^^xsd:string ;
                                   
               rdfs:subClassOf nidm:AcquisitionMethod .



