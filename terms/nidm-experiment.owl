nidm:CommonDataElement rdf:type owl:Class ;

               rdfs:label "Common Data Element" ;

               obo:IAO_0000115 "A common data element is a data element that is part of a collection of data elements, curated or not, which are available for reuse."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                 
               rdfs:subClassOf nidm:DataElement .



###  http://purl.org/nidash/nidm#ContinuousArterialSpinLabeling

nidm:ContinuousArterialSpinLabeling rdf:type owl:Class ;
                                   
               rdfs:label "Continuous Arterial Spin Labeling"^^xsd:string ;

               obo:IAO_0000115 "Continuous arterial spin labeling is a magnetic resonance imaging acquisition method that uses continuous irradiation to invert the magnetization of flowing spins to measure perfusion."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "CASL"^^xsd:string ;
                                   
               rdfs:subClassOf nidm:ArterialSpinLabeling .



###  http://purl.org/nidash/nidm#CurrentAmplifier

nidm:CurrentAmplifier rdf:type owl:Class ;
                  
               rdfs:label "Current Amplifier" ;

               rdfs:subClassOf nidm:DataProcessingDevice ;

               obo:IAO_0000115 "A current amplifier is a device that increases the electrical current amplitude of an input electrical signal by a fixed amount, while keeping the voltage constant." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .                    



###  http://purl.org/nidash/nidm#CurrentClamp

nidm:CurrentClamp rdf:type owl:Class ;
                  
               rdfs:label "Current Clamp" ;

               rdfs:subClassOf nidm:AcquisitionMethod ;

               obo:IAO_0000115 "A current clamp is an intracellular electrophysiology acquisition method in which current is injected into a cell and the voltage generated by the cell is recorded." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .                    



###  http://purl.org/nidash/nidm#DataAcquisitionDevice

nidm:DataAcquisitionDevice rdf:type owl:Class ;
                  
               rdfs:label "Data Acquisition Device" ;

               rdfs:subClassOf sio:SIO_000956 ;

               obo:IAO_0000115 "A data acquisition device is a device that collects a packet or stream of analog or digital information from an entity." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .  



###  http://purl.org/nidash/nidm#DataElement

nidm:DataElement rdf:type owl:Class ;

               rdfs:label "Data Element" ;

               obo:IAO_0000115 "A data element is a variable, used in a scientific study, used to store either information regarding the acquisition, processing, or analysis of the acquisition object that resulted from the study activity."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                 
               rdfs:subClassOf sio:SIO_000367 .



###  http://purl.org/nidash/nidm#DataProcessing

nidm:DataProcessing rdf:type owl:Class ;

               rdfs:label "Data Processing" ;

               obo:IAO_0000115 "Data processing is an activity in which data is tranformed from its initial state at acquisition to a different state more amenable to analysis."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                 
               rdfs:subClassOf prov:Activity .



###  http://purl.org/nidash/nidm#DataProcessingDevice

nidm:DataProcessingDevice rdf:type owl:Class ;
                  
               rdfs:label "Data Processing Device" ;

               rdfs:subClassOf sio:SIO_000956 ;

               obo:IAO_0000115 "A data processing device is a device that manipulates a data packet, stream, or file by performing an operation on the data." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .  



###  http://purl.org/nidash/nidm#Date

nidm:Date rdf:type owl:Class ;
                  
               rdfs:label "Date" ;

               rdfs:subClassOf sio:SIO_000015 ;

               obo:IAO_0000115 "A date is an information content entity compfising the combination of year, month, and numerical value for the day in that month that an event happened." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#DemographicsInstrument

nidm:DemographicsInstrument rdf:type owl:Class ;

               rdfs:label "Demographics Instrument"^^xsd:string ;

               obo:IAO_0000115 "A demographics instrument is a instrument usage type that signifies that the type of data to be collected by the instrument is demographics-related data."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "Common demographics information are variables such as age, handedness, phenotypic sex, ethnicity, and race." ;
               
               rdfs:subClassOf nidm:InstrumentUsageType .



###  http://purl.org/nidash/nidm#DICOMTagCollection

nidm:DICOMTagCollection rdf:type owl:Class ;
                        
               rdfs:label "DICOM Tag Collection"^^xsd:string ;
                        
               rdfs:subClassOf prov:Collection ;
                        
               obo:IAO_0000115 "A DICOM tag collection is a group of identifiers and their values that are stored in the headers of DICOM-formatted acquisition objects and that record the acquisition and reconstruction parameters for that acquisition object."^^xsd:string ;
                        
               obo:IAO_0000114 obo:IAO_0000428 ;
                        
               prov:wasAssociatedWith nidm:AcquisitionObject .



###  http://purl.org/nidash/nidm#DiffusionTensor

nidm:DiffusionTensor rdf:type owl:Class ;
                     
               rdfs:label "Diffusion Tensor"^^xsd:string ;

               obo:IAO_0000115 "Diffusion tensor is an image usage type that denotes that the acquisition object collected can be used to construct the diffusion tensor of the imaged tissue."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "DT"^^xsd:string ;
                     
               rdfs:subClassOf nidm:ImageUsageType .



###  http://purl.org/nidash/nidm#DiffusionWeighted

nidm:DiffusionWeighted rdf:type owl:Class ;
                       
               rdfs:label "Diffusion Weighted"^^xsd:string ;

               obo:IAO_0000115 "Diffusion weighted is an image contrast type in which differences in voxel intensity are based on the brownian-motion behavior of the species that generates the detected signal from that voxel."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "DW"^^xsd:string ;
                       
               rdfs:subClassOf nidm:ImageContrastType .



###  http://purl.org/nidash/nidm#DistortionCorrection

nidm:DistortionCorrection rdf:type owl:Class ;

               rdfs:label "DistortionCorrection" ;

               obo:IAO_0000115 "Distortion correction is a description of the usage of a given acquisition object, indicating that the purpose of the object is to provide information used to correct distortions in a set of images."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                
               rdfs:subClassOf nidm:ImageUsageType .



###  http://purl.org/nidash/nidm#DynamicContrastEnhanced

nidm:DynamicContrastEnhanced rdf:type owl:Class ;
                                
               rdfs:label "Dynamic Contrast Enhanced"^^xsd:string ;

               obo:IAO_0000115 "Dynamic contrast enhanced is a magnetic resonance imaging acquisition method in which T1-weighted images are acquired both before and then sequentially after a contrast agent has been injected in order to quantify metrics related to the permeability of the blood-brain barrier."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "DCE"^^xsd:string ;
                                
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#DynamicSusceptibilityContrast

nidm:DynamicSusceptibilityContrast rdf:type owl:Class ;
                                   
               rdfs:label "Dynamic Susceptibility Contrast"^^xsd:string ;

               obo:IAO_0000115 "Dynamic sysceptibility contrast is a magnetic resonance imaging acquisition method in which T2*-weighted images are acquired sequentially after a contrast agent has been injected in order to quantify metrics related to blood volume and flow."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "DSC"^^xsd:string ;
                                   
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#EchoPlanar

nidm:EchoPlanar rdf:type owl:Class ;
                
               rdfs:label "Echo Planar"^^xsd:string ;
                
               rdfs:subClassOf nidm:k-spaceTraversalScheme ;
                
               obo:IAO_0000115 "Echo planar is a k-space traversal scheme  used to acquire the k-space data in magnetic resonance imaging in which the entirety of k-space is continuously traversed for each repetition of the pulse sequence."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Electrocardiography

nidm:Electrocardiography rdf:type owl:Class ;

               obo:IAO_0000115 "Electrocardiography (ECG) is an electrophysiological acquisition method in which electrodes placed on the skin measure the electrical activity of the heart."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                          
               rdfs:label "Electrocardiography"^^xsd:string ;
                          
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Electrocorticography

nidm:Electrocorticography rdf:type owl:Class ;

               obo:IAO_0000115 "Electrocorticography (ECoG) is an electrophysiological acquisition method in which the electrical activity of the brain is measured using electrodes that are placed directly on the cortical surface of the brain."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                          
               rdfs:label "Electrocorticography"^^xsd:string ;
                          
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Electrode

nidm:Electrode rdf:type owl:Class ;

               rdfs:label "Electrode"^^xsd:string ;

               obo:IAO_0000115 "An electrode is a data acquisition device that consists of a conductor that is used to record an electrical signal from a study participant."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;
               
               rdfs:subClassOf nidm:DataAcquisitionDevice .



###  http://purl.org/nidash/nidm#Electroencephalography

nidm:Electroencephalography rdf:type owl:Class ;

               obo:IAO_0000115 "Electroencephalography (EEG) is an electrophysiological acquisition method in which the electrical activity of the brain is measured using electrodes that are placed on the surface of the head."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                            
               rdfs:label "Electroencephalography"^^xsd:string ;
                            
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#ElectroencephalographyAcquisitionDevice

nidm:ElectroencephalographyAcquisitionDevice rdf:type owl:Class ;

               obo:IAO_0000115 "An electroencephalography (EEG) acquisition device is a device that measures the electrical activity of the brain and consists of a set of electrodes placed on the surface of the head and a recording device that collects the signal from each electrode."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                            
               rdfs:label "Electroencephalography Acquisition Device"^^xsd:string ;
                            
               rdfs:subClassOf nidm:DataAcquisitionDevice .



###  http://purl.org/nidash/nidm#Electromyography

nidm:Electromyography rdf:type owl:Class ;

               obo:IAO_0000115 "Electromyography (EMG) is an electrophysiological acquisition method in which the electrical activity of the muscles is measured using either surface or inserted electrodes."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                          
               rdfs:label "Electromyography"^^xsd:string ;
                          
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Electrooculography

nidm:Electrooculography rdf:type owl:Class ;

               obo:IAO_0000115 "Electrooculography (EOG) is an electrophysiological acquisition method in which the resting potential between the cornea of the eye and Bruch's membrance is measured."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                            
               rdfs:label "Electrooculography"^^xsd:string ;
                            
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#ElectrophysiologyRecording

nidm:ElectrophysiologyRecording rdf:type owl:Class ;
                                             
               rdfs:label "Electrophysiology Recording"^^xsd:string ;

               obo:IAO_0000115 "Electrophysiology Recording is an acquisition modality in which electrical signals are recorded from one or more electrodes inserted into living tissue."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                                             
               rdfs:subClassOf nidm:AcquisitionModality .



###  http://purl.org/nidash/nidm#Encephalography

nidm:Encephalography rdf:type owl:Class ;

               obo:IAO_0000115 "Encephalography is an acquisition modality in which the electrical activity or structure of the brain is measured using a variety of techniques."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                            
               rdfs:label "Electroencephalography"^^xsd:string ;
                            
               rdfs:subClassOf nidm:AcquisitionModality .



###  http://purl.org/nidash/nidm#NIDM_0000153

nidm:NIDM_0000153 rdf:type owl:Class ;
                  
               rdfs:label "Event-Related Design"^^xsd:string ;
                  
               obo:IAO_0000115 "An event-related design is a fMRI design comprised of discrete events of stimulus and/or behavior."^^xsd:string ;

               rdfs:subClassOf nidm:NIDM_0000155 ;
                  
               obo:IAO_0000116 "Discussed at https://github.com/incf-nidash/nidm/issues/299"^^xsd:string ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ExtracellularElectrophysiologyRecording

nidm:ExtracellularElectrophysiologyRecording rdf:type owl:Class ;
                                             
               rdfs:label "Extracellular Electrophysiology Recording" ;

               obo:IAO_0000115 "Extracellular Electrophysiology Recording is an acquisition modality in which one or more electrodes are inserted into living tissue and the electrical signals from adjacent cells are recorded."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                                             
               rdfs:subClassOf nidm:ElectrophysiologyRecording .



###  http://purl.org/nidash/nidm#ExVivo

nidm:ExVivo rdf:type owl:Class ;

               rdfs:label "Ex Vivo" ;

               obo:IAO_0000115 "Ex vivo is a organismal quality, usually used as an attribute of an Agent, that indicates that the Agent it describes was once part of a living organism, but has been removed."^^xsd:string ;

	       rdfs:comment "A common example is a tissue sample."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                
               rdfs:subClassOf obo:PATO_0001995 .



###  http://purl.org/nidash/nidm#EyeTrackingDevice

nidm:EyeTrackingDevice rdf:type owl:Class ;
                    
               rdfs:label "Eye Tracking Device" ;

               obo:IAO_0000115 "An eye tracking device is a device that consists of hardware that follows the motion of one or both eyes of the study participant, which allows the determination of either the gaze location or of relative eye motion."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                    
               rdfs:subClassOf nidm:DataAcquisitionDevice .



###  http://purl.org/nidash/nidm#FastFieldEcho

nidm:FastFieldEcho rdf:type owl:Class ;

               rdfs:label "Fast Field Echo"^^xsd:string ;

               obo:IAO_0000115 "Fast Field Echo is a magnetic resonance imaging acquisition method that employs a gradient to dephase and rephase the signal to create an echo, a gradient-recalled echo, which is then measured."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "FFE"^^xsd:string ;

               rdfs:comment "Note that FFE is the Philips name for their implementation of the gradient recalled echo pulse sequence."^^xsd:string ;

               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#FastImagingEmployingSteadyStateAcquisition

nidm:FastImagingEmployingSteadyStateAcquisition rdf:type owl:Class ;
                                
               rdfs:label "Fast Imaging Employing Steady State Acquisition"^^xsd:string ;

               obo:IAO_0000115 "Fast imaging employing steady state acquisition (FIESTA) is a gradient recalled echo based magnetic resonance imaging acquisition method in which a series of phase-alternating radiofrequency pulses are applied, in conjunction with a balanced set of gradients, generating free induction decays and a steady-state transverse magnetization, the signal from which is collected."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "FIESTA"^^xsd:string ;

	       rdfs:comment "FIESTA is the name given to this sequence by GE, True FISP is the acronym used by Siemens."^^xsd:string ;
	       
               rdfs:subClassOf nidm:SteadyStateFreePrecession .



###  http://purl.org/nidash/nidm#FastImagingWithSteadyStatePrecession

nidm:FastImagingWithSteadyStatePrecession rdf:type owl:Class ;
                                
               rdfs:label "Fast Imaging With Steady State"^^xsd:string ;

               obo:IAO_0000115 "Fast imaging with steady state precession (FISP) is a gradient recalled echo based magnetic resonance imaging acquisition method in which a series of phase-alternating radiofrequency pulses are applied, in conjunction with spoiler and rewinder gradients, generating free induction decays and a steady-state transverse magnetization, the signal from which is collected."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "FISP"^^xsd:string ;

	       rdfs:comment "GRASS is the name given to this sequence by GE, FISP is the acronym used by Siemens."^^xsd:string ;
	       
               rdfs:subClassOf nidm:SteadyStateFreePrecession .




###  http://purl.org/nidash/nidm#FastLowAngleShot

nidm:FastLowAngleShot rdf:type owl:Class ;

               rdfs:label "Fast Low Angle Shot"^^xsd:string ;

               obo:IAO_0000115 "Fast Low Angle Shot is a magnetic resonance imaging acquisition method that employs a gradient to dephase and rephase the signal to create an echo, which is then measured, and an additional gradient to destroy any residual magnetization at the end of the pulse sequence."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "FLASH"^^xsd:string ;

	       rdfs:subClassOf nidm:SpoiledGradientRecalledEcho ;

               rdfs:comment "Note that FLASH is the Siemens name for their implementation of this pulse sequence.  GE calls their implementation Spoiled Gradient Echo (SPGR) and Philips calls theirs Contrast Enhanced or T1 Fast Field Echo (T1-FFE)"^^xsd:string .



###  http://purl.org/nidash/nidm#FastSpinEcho

nidm:FastSpinEcho rdf:type owl:Class ;
                                
               rdfs:label "Fast Spin Echo"^^xsd:string ;

               obo:IAO_0000115 "Fast spin echo is a magnetic resonance imaging acquisition method in which the echo used in the measurement is generated by application of an initial radiofrequency 90 degree pulse and multiple subsequent 180 degree pulses, the latter in conjunction with a different phase-encoding gradient at each refocussed echo."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "FSE"^^xsd:string ;

	       rdfs:comment "FSE is also known as turbo spin echo"^^xsd:string ;
                                
               rdfs:subClassOf nidm:SpinEcho .



###  http://purl.org/nidash/nidm#FieldPotential

nidm:FieldPotential rdf:type owl:Class ;
                    
               rdfs:label "Field Potential" ;

               obo:IAO_0000115 "The field potential is an extracellular electrophysiology acquisition method in which an electrode is used to measure the voltage that is generated by synaptic activity (in neural tissue)."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                    
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#FilePath

nidm:FilePath rdf:type owl:Class ;
                  
               rdfs:label "File Path"^^xsd:string ;

               obo:IAO_0000115 "A file path is the location of a file on a computer storage volume."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                  
               rdfs:subClassOf nidm:Path .



###  http://purl.org/nidash/nidm#FlowWeighted

nidm:FlowWeighted rdf:type owl:Class ;
                  
               rdfs:label "Flow Weighted"^^xsd:string ;

               obo:IAO_0000115 "Flow weighted is an image contrast type in which differences in voxel intensity are based on the bulk motion behavior of the species that generates the detected signal from that voxel."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                  
               rdfs:subClassOf nidm:ImageContrastType .



###  http://purl.org/nidash/nidm#FluidAttenuatedInversionRecovery

nidm:FluidAttenuatedInversionRecovery rdf:type owl:Class ;
                                   
               rdfs:label "Fluid Attenuated Inversion Recovery"^^xsd:string ;

               obo:IAO_0000115 "Fluid Attenuated Inversion Recovery, which is often known by its acronym, FLAIR, is a magnetic resonance imaging acquisition method in which images are acquired after an initial magnetization inversion pulse is applied and a delay duration that is selected such that the signal from bulk fluid such as cerebral spinal fluid or fluid in lesions is nulled."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "FLAIR"^^xsd:string ;
                                   
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Functional

nidm:Functional rdf:type owl:Class ;

               rdfs:label "Functional"^^xsd:string ;

               obo:IAO_0000115 "Functional is a description of the usage of a given acquisition object, indicating that the purpose of the object is to provide information on the biological function of some aspect of the object on which the acquisition was performed."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                
               rdfs:subClassOf nidm:ImageUsageType .



###  http://purl.org/nidash/nidm#GradientRecalledAcquisitionInSteadyState

nidm:GradientRecalledAcquisitionInSteadyState rdf:type owl:Class ;
                                
               rdfs:label "Gradient Recalled Acquisition in Steady State"^^xsd:string ;

               obo:IAO_0000115 "Gradient recalled acquisition in steady state (GRASS) is a gradient recalled echo based magnetic resonance imaging acquisition method in which a series of phase-alternating radiofrequency pulses are applied, generating free induction decays and a steady-state transverse magnetization, the signal from which is collected."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "GRASS"^^xsd:string ;

	       rdfs:comment "GRASS is the name given to this sequence by GE, FISP is the acronym used by Siemens."^^xsd:string ;
	       
               rdfs:subClassOf nidm:SteadyStateFreePrecession .



###  http://purl.org/nidash/nidm#GradientRecalledEcho

nidm:GradientRecalledEcho rdf:type owl:Class ;
                                
               rdfs:label "Gradient Recalled Echo"^^xsd:string ;

               obo:IAO_0000115 "Gradient recalled echo is a magnetic resonance imaging acquisition method in which the echo used in the measurement is generated by application and subsequent reversal of the frequency-encoding gradient and is characterized by flip angles of less than 90 degrees and the evolution being governed by T2star decay."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;

	       nidm:hasAcronym "GRE"^^xsd:string ;
                                
